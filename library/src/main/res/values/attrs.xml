    <declare-styleable name="ItemView">
        <attr name="titleText" format="string"></attr>
        <attr name="titleStyle" format="integer"></attr>
        <attr name="length" format="integer"></attr>
        <attr name="inputType" format="integer"></attr>
        <attr name="cellHeight" format="dimension"></attr>
        <attr name="drawablePad" format="dimension"></attr>
        <attr name="padingLeft" format="dimension"></attr>
        <attr name="titleTextSize" format="dimension"></attr>
